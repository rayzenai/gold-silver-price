{
    "name": "rayzenai/gold-silver-price",
    "description": "Laravel package for fetching and managing gold and silver prices with Filament integration",
    "keywords": ["laravel", "gold", "silver", "price", "filament", "nepal"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiran Timsina",
            "email": "to@kiran.work"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^11.0|^12.0",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "pestphp/pest": "^3.0|^4.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "RayzenAI\\GoldSilverPrice\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RayzenAI\\GoldSilverPrice\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RayzenAI\\GoldSilverPrice\\GoldSilverPriceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
